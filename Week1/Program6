#include <iostream>
using namespace std;

int main() {
    float f, c;
    cout << "Enter the temperature in Fahrenheit: "<< endl;
    cin >> f;
    c = (f - 32) * 5.0 / 9.0;
    cout << "Temperature in Celsius is: " << c << "C" << endl;
    cout << "Enter the temperature in Celsius: " << endl;
    cin >> c;
    f = (c * 9.0 / 5.0) + 32;
    cout << "Temperature in Fahrenheit is: " << f << "F" << endl;
    return 0;
}
