#include <iostream>

using namespace std;

void generate Fibonacci(int n) {

int a=0,b=1;

for(int i=0;i<n;i++) { cout<<a<<" "; int next=a+b; a=b; b=next; }

int main() {

int n;

cout<<"Enter the number of terms:";

cin>>n;

cout<<"Fibonacci sequence:"<<endl;

generate Fibonacci(n);

cout<<endl;

return 0;

}
